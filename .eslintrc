{
    "extends": ["airbnb-base", "prettier"],
    "rules": {
      "semi": ["error", "never"],
      "no-console": "off"
    },
    "globals": {
      "afterAll": true,
      "afterEach": true,
      "beforeAll": true,
      "beforeEach": true,
      "describe": true,
      "expect": true,
      "fit": true,
      "it": true,
      "jasmine": true,
      "xit": true,
      "jest": true,
      "test": true
    },
    "settings": {
      "plugins": ["import"],
      "rules": {
        "import/no-unresolved": "error",
        "import/no-extraneous-dependencies": 0
      }
    }
  }
  